{
  "name": "plugin-marketplace-publisher",
  "version": "1.0.0",
  "description": "Publisher Dashboard for Plugin Marketplace",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server-mock\" \"node scripts/start-frontend.js\"",
    "server": "nodemon --exec \"npx ts-node --project tsconfig.script.json\" src/server.ts",
    "server-simple": "nodemon --exec \"npx ts-node --project tsconfig.script.json\" src/server-simple.ts",
    "server-mock": "nodemon --exec \"npx ts-node --project tsconfig.script.json\" src/server-mock.ts",
    "client": "vite",
    "build": "vite build",
    "start": "node dist/server.js",
    "db:reset": "npx ts-node --project tsconfig.script.json src/scripts/resetDatabase.ts",
    "db:sync": "npm run typeorm -- schema:sync",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.1.3",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-vue-next": "^0.546.0",
    "pg": "^8.13.1",
    "pinia": "^2.2.6",
    "tailwindcss": "^3.4.14",
    "typeorm": "^0.3.20",
    "vue": "^3.5.12",
    "vue-chartjs": "^5.3.1",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.28.0",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.47",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "~5.6.3",
    "vite": "^5.4.9",
    "vue-tsc": "^2.1.6",
    "wait-on": "^9.0.1"
  }
}
